{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    
    "lib": ["es6"],
    "outDir": "./dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "src/**/*.ts",
    "src/database/**/*.ts"
  ]
}
