{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "CommonJS",
    "paths": {
      "@/*": ["src/*"],
    },
    "lib": ["es6"],
    "outDir": "./dist",
    "strict": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "src/**/*.ts",
    "src/database/**/*.ts"
, "src/server.ts"  ]
}
